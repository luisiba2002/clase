//Lo he hecho muy simple y con if anidados


#include <iostream>

using namespace std;

int main()
{
    float nota;
    cout<<"Dame una nota entre el 1 y el 10" <<endl;
    cin>>nota;
    if (nota<5)
    {
        cout<<"Suspenso";
    }
    else
    {
        if (nota<7)
        {
            cout<<"Aprobado";
        }
        else
        {
            if (nota<9)
            {
                cout<<"Notable";
            }
            else
            {
                cout<<"Sobresaliente";
            }
        }
    }
    return 0;
}
